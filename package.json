{
	"name": "search-and-retina",
	"displayName": "Search and Retina",
	"description": "Transform files through Retina scripts",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:retina.helloWorld",
		"onCommand:retina.runOnActiveDocument"
	],
	"main": "./out/retina.js",
	"contributes": {
		"commands": [
			{
				"category": "Retina",
				"command": "retina.helloWorld",
				"title": "Hello World"
			},
			{
				"category": "Retina",
				"command": "retina.runOnActiveDocument",
				"title": "Run On Active Document"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}